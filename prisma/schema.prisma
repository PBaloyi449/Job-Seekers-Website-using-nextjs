generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Entry_level_Jobs {
  id               String @id @default(auto()) @map("_id") @db.ObjectId
  closingDate      String
  company          String
  description      String
  job              String
  link             String
  location         String
  requirements     String
  responsibilities String

  @@map("Entry-level-Jobs")
}
