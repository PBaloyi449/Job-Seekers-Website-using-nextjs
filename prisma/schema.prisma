generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Entry_level_Job {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  apply            String
  date             String   
  company          String
  description      String
  location         String
  name             String
  requirements     String
  responsibilities String
  postedAt         DateTime @default(now())

  @@map("Entry-level Jobs")
}
